<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itheima.mapper.OrdersMapper">
    <select id="selectByid" resultType="Orders">
        select *
        from tb_orders
        where user_id = #{id}
    </select>

</mapper>